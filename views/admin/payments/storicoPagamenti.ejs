<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <section class="cards">
        <% pagamenti.forEach(pagamento => { %>
            <div class="card">
                <h2>Data: <%= pagamento.data %></h2>
                <h2>Importo: <%= pagamento.importo %></h2>
            </div>
        <% }); %>
    </section>
</body>
</html>